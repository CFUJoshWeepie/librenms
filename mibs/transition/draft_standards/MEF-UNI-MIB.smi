-- *****************************************************************************
-- Ethernet Service Class of Service Profile Table
-- *****************************************************************************

mefServiceCosNextIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object contains an unused value for a Class of Service (CoS)
        profile on a MEF compliant device, or a zero to indicate that none
        exist. This value needs to be read in order to find an available index
        for row-creation of a CoS profile and then used when a row is created.
        This value is automatically updated by the SNMP Agent after the row is
        created.

        Referential integrity is necessary, i.e., the index needs to be
        persistent upon a reboot or restart of a device.  The index
        is never to be reused for a new bandwidth profile on the same MEF compliant
        device until it wraps to zero.  The index value keeps
        increasing up to that time. This is to facilitate access control based
        on a fixed index for an EMS, since the index is not reused.
       "
    ::= { mefServiceCosAttributes 1 }

mefServiceCosCfgTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MefServiceCosCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "This table supports Class of Service profile settings."
    ::= { mefServiceCosAttributes 2 }

mefServiceCosCfgEntry OBJECT-TYPE
    SYNTAX      MefServiceCosCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
       "Class of Service settings table entry."
    INDEX { mefServiceCosCfgIndex }
    ::= { mefServiceCosCfgTable 1 }

MefServiceCosCfgEntry ::= SEQUENCE {
      mefServiceCosCfgIndex             Unsigned32,
      mefServiceCosCfgIdentifier        OCTET STRING,
      mefServiceCosCfgType              INTEGER,
      mefServiceCosCfgIdentifierList    OCTET STRING,
      mefServiceCosCfgRowStatus         RowStatus
    }

mefServiceCosCfgIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Class of Service profile index number."
    ::= { mefServiceCosCfgEntry 1 }

mefServiceCosCfgIdentifier OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(1..45))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "This object indicates the Class of Service profile identifier for the
        associated CoS profile index.
       "
    REFERENCE
       "[MEF 10.2]"
    ::= { mefServiceCosCfgEntry 2 }

mefServiceCosCfgType OBJECT-TYPE
    SYNTAX      INTEGER {
        uni             (1),
        evc             (2),
        pcp             (3),
        dscp            (4),
        l2cp            (5)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Sets the CoS type and indicates the interpretation of the
         mefServiceCosIdentifierList object."
    ::= { mefServiceCosCfgEntry 3 }

mefServiceCosCfgIdentifierList OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(1..256))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "This object indicates the list of CoS identifiers associated with
        the specific CoS profile. Interpretation of the values in the list
        are based upon the mefServiceCosCfgType object.

        CoS identifiers can be a single value. Individual values are separated
        by a comma, ','. A range of CoS identifiers are indicated by two values
        separated by '..'.
       "
    REFERENCE
       "[MEF 10.2]"
    ::= { mefServiceCosCfgEntry 4 }

mefServiceCosCfgRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "The status of the row.

        The writable columns in a row cannot be changed if the row
        is active. All columns must have a valid value before a row
        can be activated.
       "
   ::= { mefServiceCosCfgEntry 5 }



